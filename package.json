{
    "name": "vscode-doxyfile",
    "displayName": "Doxyfile",
    "description": "Doxyfile language support",
    "icon": "resources/doxygen_icon.png",
    "version": "1.0.2",
    "publisher": "samubarb",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/samubarb/vscode-doxyfile.git"
    },
    "engines": {
        "vscode": "*"
    },
    "categories": [
        "Programming Languages"
    ],
    "keywords": [
        "doxygen",
        "doxyfile"
    ],
    "contributes": {
        "languages": [
            {
                "id": "doxyfile",
                "aliases": [
                    "Doxyfile",
                    "doxyfile"
                ],
                "extensions": [
                    ".Doxyfile"
                ],
                "filenames": [
                    "Doxyfile"
                ],
                "filenamePatterns": [
                    "Doxyfile.*"
                ],
                "configuration": "./language-configuration.json",
                "icon": {
                    "light": "./resources/doxygen_icon_light.svg",
                    "dark": "./resources/doxygen_icon_dark.svg"
                },
				"firstLine": "# Doxyfile"
            }
        ],
        "grammars": [
            {
                "language": "doxyfile",
                "scopeName": "source.doxyfile",
                "path": "./syntaxes/doxyfile.tmLanguage.json"
            }
        ]
    }
}
